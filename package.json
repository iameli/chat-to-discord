    "@babel/preset-env": "^7.4.4",
    "cross-env": "^5.2.0",
    "esm": "^3.2.22",
    "node-dev": "^4.0.0",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5"
  },
